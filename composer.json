{
  "name": "kubeecms/custom-fields-pro",
  "type": "wordpress-plugin",
  "license": "GNU",
  "description":
    "All-in-one enhancement suite that improves Custom Fields",
  "homepage": "https://github.com/KubeeCMS/Custom-Fields-Pro/",
  "keywords": ["kubeecms", "kubee", "kcms-drive"],
  "support": {
    "issues": "https://github.com/KubeeCMS/Custom-Fields-pro/issues/",
    "forum": "https://discourse.kubee.com/"
  },
  "autoload": {
    "psr-4": {
      "Roots\\Bedrock\\": "/"
    }
  },
  "require": {
    "php": ">=7.3",
    "composer/installers": "^1.10"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
  },
  "scripts": {
    "test": ["phpcs --ignore=vendor --extensions=php --standard=PSR2 ."]
  }
}
